<% if current_user.has_role?(:admin) %>
  <%= render 'layouts/shared/adminMenu' %>
<% elsif current_user.has_role?(:staff) %>
  <%= render 'layouts/shared/staffMenu' %>
<% elsif current_user.has_role?(:student) %>
  <%= render 'layouts/shared/studentMenu' %>
<% else %>
  <%= render 'layouts/shared/noRoleMenu' %>
<% end %>